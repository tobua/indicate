{
  "name": "indicate",
  "description": "Scroll indicator for elements with scrollable overflow.",
  "version": "0.0.0-development",
  "repository": "github:tobua/indicate",
  "homepage": "https://tobua.github.io/indicate",
  "license": "MIT",
  "author": "Matthias Giger",
  "scripts": {
    "build": "padua build",
    "start": "padua watch",
    "test": "cypress open"
  },
  "padua": {
    "entry": "theme/*.ts"
  },
  "dependencies": {
    "csstype": "^3.0.11",
    "react": "^17.0.2"
  },
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./theme/*": {
      "default": "./dist/theme/*.js"
    }
  },
  "source": "index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "scroll",
    "indicator",
    "fade",
    "overflow"
  ],
  "devDependencies": {
    "@types/react": "^17.0.39",
    "cypress": "^9.5.1",
    "padua": "^0.5.3"
  },
  "prettier": "padua/configuration/.prettierrc.json",
  "eslintConfig": {
    "extends": "./node_modules/padua/configuration/eslint.cjs"
  },
  "engines": {
    "node": ">= 14"
  },
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "theme/*": [
        "dist/theme/*"
      ]
    }
  }
}
