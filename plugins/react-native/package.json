{
  "name": "react-native-indicate",
  "description": "Enhanced ScrollView with elements indicating scrollable overflow.",
  "version": "1.0.0",
  "repository": "github:tobua/indicate",
  "homepage": "https://github.com/tobua/indicate/tree/master/plugins/react-native",
  "scripts": {
    "test": "jest",
    "watch": "cpx 'src/**/*' app/node_modules/react-native-indicate/src --watch",
    "app": "node create-app.js"
  },
  "dependencies": {
    "omit.js": "^2.0.2"
  },
  "type": "module",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@react-native-community/eslint-config": "3.0.0",
    "babel-jest": "^27.0.6",
    "cpx": "^1.5.0",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-native": "^0.65.1",
    "react-test-renderer": "^17.0.2",
    "recursive-copy": "^2.0.13",
    "rimraf": "^3.0.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": "@react-native-community",
    "rules": {
      "semi": 0
    }
  },
  "jest": {
    "moduleNameMapper": {
      "react-native-indicate": "<rootDir>/src"
    },
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "/app/"
    ]
  },
  "engines": {
    "node": ">= 14"
  }
}
